@PART[SampleReturnCapsule_Parachute]:NEEDS[kOS]:AFTER[kOS]
{
	@description:NEEDS[kOS] ^= :$: <color=green>KOS, kOSSNameTag </color> :
	
	!MODULE[kOSProcessor] {
		@MODULE[kOSProcessor] {
			%name = kOSProcessor
			%bootFile = /boot/boot.ks
			%diskSpace = 125000
			%ECPerBytePerSecond = 0
			%ECPerInstruction = 0.000004
		}

		@MODULE[KOSNameTag] {
			%nameTag = sample command
		}
}

